
#дз номер 6:

A = [[[5, 3, -3],[-4, 3, 0], [-4, -6, 3]],
     [[2, 2, -5], [-2, 4, 3], [3, 5, -2]],
     [[3, 2, 5], [2, -2, -5], [-3, -6, 2]]]
     
B = [[2, 3, 2],[6 , -4, 1],[-5 , 1, 2]]

C = [[-2, 0, -1], [2, 1, -2], [4, 6, 2]]
for i in range(3):
    summ = 0
    for n in range(3):
        for t in range(3):
            for m in range(3):
                    summ += A[n][t][m] * B[n][t] * C[m][i]
    print(summ)
    
    
    
    
